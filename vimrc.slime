Bundle "jpalardy/vim-slime"

let g:slime_target = "tmux"
"let g:slime_paste_file = "$HOME/.slime_paste"
let g:slime_config = {"socket_name": "default", "target_pane": ":1.2"}
let b:slime_config = {"socket_name": "default", "target_pane": ":1.2"}

let g:slime_no_mappings = 1
map <C-f> <Plug>SlimeLineSend
map <C-g> <Plug>SlimeRegionSend
